{
  "name": "automation-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint",
    "lint": "eslint . -c .eslintrc.cjs --fix --cache",
    "format": "prettier --write . ",
    "precommit": "concurrently -g -n \"eslint,lint:next,test\" -c \"#341BAB,#A6CC66,#3077C6\" \"bun run lint\" \"bun run lint:next\" \"echo 'not implemented'\"",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "concurrently": "^9.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
